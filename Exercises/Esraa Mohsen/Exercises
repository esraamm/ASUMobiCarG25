//blinking with switch
const int LedPin =13;
const int buttonPin = 2 ; 
int val;
void setup ( )
{
  pinMode(LedPin,OUTPUT);
  pinMode(buttonPin,INPUT);
}
void loop()
{
 val=digitalRead(buttonPin);
if (val == HIGH)
 {
  digitalWrite(LedPin,HIGH);
  delay(1000);
  digitalWrite(LedPin,LOW);
  delay(1000);
 }
 else{ digitalWrite(LedPin, LOW);}
 
}
